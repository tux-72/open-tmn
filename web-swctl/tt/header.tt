<link href="/swctl/?css=main.css" rel="stylesheet" type="text/css">

<script language="javascript" type="text/javascript">
function toggleDisplay(e){
    element = document.getElementById(e).style;
    element.display == 'none' ? element.display = 'block' : element.display='none';
    if( document.getElementById(e).width == '100%')
        document.getElementById(e).width = document.getElementById(e+'_w').clientWidth+10;
}

function conf_del(p,u){
    if( confirm( 'Delete ' + p ) )
        window.location.href = u;
}

function toggleDisplay_all(r){
    var t = document.getElementsByTagName('table')
    for(var i=0; i<t.length; i++){
        if(t[i].id.match(r))
            toggleDisplay(t[i].id)
    }
}
</script>

</head>
<body>
[% IF err %] <h1 align=center><font color=red> [% err %]</font></h1>[% END %]
